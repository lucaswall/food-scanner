{
  "mcpServers": {
    "linear": {
      "type": "http",
      "url": "https://mcp.linear.app/mcp"
    },
    "Railway": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@railway/mcp-server"
      ],
      "env": {}
    },
    "fitbit": {
      "type": "stdio",
      "command": "npx",
      "args": ["tsx", "mcp-fitbit/index.ts"]
    }
  }
}
